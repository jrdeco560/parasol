{
    "pipeline": [
        {
            "type": "readers.las",
            "filename": "input.laz"
        },  
        {
            "type": "filters.crop",
            "bounds": "( [-71.2771291,-71.27433599999999],[42.3171413,42.31989568])"
        },
        {
            "type": "filters.reprojection",
            "out_srs": "EPSG:32619"
        },
        {
            "type":"filters.voxelgrid",
            "leaf_x": 1.0,
            "leaf_y": 1.0, 
            "leaf_z": 1.0 
        },
        {
            "type": "filters.outlier",
            "method": "statistical",
            "mean_k": 12,
            "multiplier": 2.2
        },
        {
            "type":"filters.range",
            "limits":"Classification![7:7]"
        },
        {
            "type": "filters.normal",
            "knn": 8, 
            "tag": "ALL"
        },
        {
            "type": "writers.las", 
            "inputs": ["ALL"], 
            "filename": "all.laz",
            "scale_x": "auto",
            "scale_y": "auto",
            "scale_z": "auto",
            "offset_x": "auto",
            "offset_y": "auto",
            "offset_z": "auto",
            "compression": "laszip"
        },
        {
            "type": "writers.text",
            "inputs": ["ALL"], 
            "format": "csv",
            "order": "X,Y,Z,NormalX,NormalY,NormalZ", 
            "keep_unspecified": "false", 
            "filename": "all.csv"
        },
        {
            "type": "filters.pmf", 
            "inputs": ["ALL"] 
        },
        {
            "type": "filters.range",
            "limits": "Classification[2:2]", 
            "tag": "GROUND"
        },
        {
            "type": "writers.las", 
            "inputs": ["GROUND"], 
            "filename": "grnd.laz",
            "scale_x": "auto",
            "scale_y": "auto",
            "scale_z": "auto",
            "offset_x": "auto",
            "offset_y": "auto",
            "offset_z": "auto",
            "compression": "laszip"
        },
        {
            "type": "writers.text",
            "inputs": ["GROUND"], 
            "format": "csv",
            "order": "X,Y,Z,NormalX,NormalY,NormalZ", 
            "keep_unspecified": "false", 
            "filename": "grnd.csv"
        }
    ]
}
