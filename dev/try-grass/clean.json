{
    "pipeline": [
        {
            "type": "readers.las",
            "filename": "input.laz"
        },  
        {
            "type": "filters.reprojection",
            "out_srs": "EPSG:32619"
        },
        {
            "type": "filters.outlier",
            "method": "statistical",
            "mean_k": 12,
            "multiplier": 2.2
        },
        {
            "type":"filters.range",
            "limits":"Classification![7:7]"
        },
        {
            "type": "writers.las", 
            "filename": "clean.laz",
            "a_srs": "EPSG:32619", 
            "scale_x": "auto",
            "scale_y": "auto",
            "scale_z": "auto",
            "offset_x": "auto",
            "offset_y": "auto",
            "offset_z": "auto",
            "compression": "laszip"
        }
    ]
}
